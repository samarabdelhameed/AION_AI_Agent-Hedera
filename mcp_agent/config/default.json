{
  "server": {
    "port": 3001,
    "host": "localhost",
    "timeout": 30000,
    "cors": {
      "enabled": true,
      "origins": ["http://localhost:3000", "http://localhost:5173"],
      "credentials": true
    }
  },
  "database": {
    "host": "localhost",

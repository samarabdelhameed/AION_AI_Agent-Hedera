{
  "server": {
    "port": 3001,
    "host": "0.0.0.0",
    "timeout": 30000,
    "cors": {
      "enabled": true,
      "origins": [
        "https://aion-ai.com",
        "https://app.aion-ai.com"
      ],
      "credentials": true
    }
  },
  "security": {
    "jwtSecret": "production_jwt_secret_change_me",
    "jwtExpiresIn": "24h",
    "bcryptRounds": 12,
    "rateLimitWindow": 60000,
    "rateLimitMax": 50,
    "corsOrigins": [
      "https://aion-ai.com"
    ],
    "trustedProxies": [
      "127.0.0.1"
    ]
  },
  "logging": {
    "level": "info",
    "format": "json",
    "file": "./logs/mcp_agent_production.log",
    "console": false,
    "maxFiles": 10,
    "maxSize": "50m"
  },
  "hedera": {
    "network": "testnet",
    "services": {
      "hcs": {
        "enabled": true,
        "topicId": null
      },
      "hfs": {
        "enabled": true,
        "bridgeFileId": null
      },
      "hts": {
        "enabled": true,
        "shareTokenId": null
      },
      "hscs": {
        "enabled": true,
        "vaultContractId": null
      }
    },
    "accounts": {
      "operator": {
        "accountId": null,
        "privateKey": null
      }
    },
    "monitoring": {
      "enabled": true
    }
  },
  "ai": {
    "model": {
      "version": "v2.1.0",
      "confidenceThreshold": 0.85
    },
    "decisionLogging": {
      "enabled": true,
      "logToHCS": true
    },
    "modelMetadata": {
      "storeToHFS": true
    }
  }
}
{
  "name": "aion-defi-platform",
  "version": "1.0.0",
  "description": "AION - The Immortal AI DeFi Agent on BNBChain",
  "main": "index.js",
  "scripts": {
    "dev": "./scripts/start-dev.sh",
    "start": "./scripts/start-dev.sh",
    "start:mcp": "cd mcp_agent && npm start",
    "start:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm ci && npm run build",
    "vercel-build": "cd frontend && npm ci && npm run build",
    "test": "npm run test:mcp && npm run test:frontend",
    "test:mcp": "cd mcp_agent && npm test",
    "test:frontend": "cd frontend && npm test",
    "test:real-data": "cd mcp_agent && npm run test:real-data",
    "test:smart-contracts": "cd mcp_agent && npm run test:smart-contracts",
    "test:stress": "cd mcp_agent && npm run test:stress",
    "test:comprehensive": "cd mcp_agent && npm run test:comprehensive",
    "install:all": "npm install && cd mcp_agent && npm install && cd ../frontend && npm install",
    "clean": "rm -rf node_modules mcp_agent/node_modules frontend/node_modules",
    "clean:logs": "rm -rf logs/*",
    "status": "./scripts/health-check.sh",
    "stop": "./scripts/stop-dev.sh",
    "restart": "./scripts/stop-dev.sh && ./scripts/start-dev.sh",
    "logs": "tail -f logs/*.log",
    "logs:mcp": "tail -f logs/mcp_agent.log",
    "logs:frontend": "tail -f logs/frontend.log",
    "setup:hedera": "node scripts/setup-verification-environment.js",
    "verify:hedera": "node scripts/generate-hedera-verification-links.js",
    "validate:env": "node scripts/validate-environment.js",
    "test:hedera": "node scripts/test-hedera-connection.js",
    "create:accounts": "node scripts/create-test-accounts.js",
    "setup:complete": "node scripts/setup-complete-infrastructure.js",
    "generate:verification": "node scripts/generate-hedera-verification-links.js",
    "deploy:hedera": "node scripts/deploy-hedera-contracts.js",
    "create:hts": "node scripts/create-real-hts-token.js",
    "mint:hts": "node scripts/execute-hts-mint-operations.js",
    "burn:hts": "node scripts/execute-hts-burn-operations.js",
    "create:hcs": "node scripts/create-hcs-ai-topic.js",
    "submit:ai": "node scripts/submit-real-ai-decisions.js",
    "validate:hcs": "node scripts/validate-hcs-messages.js",
    "create:metadata": "node scripts/create-real-model-metadata.js",
    "store:hfs": "node scripts/store-metadata-on-hfs.js",
    "cross:reference": "node scripts/cross-reference-hfs-hcs.js"
  },
  "keywords": [
    "defi",
    "ai",
    "blockchain",
    "bnbchain",
    "yield-farming",
    "smart-contracts",
    "mcp-agent",
    "react",
    "typescript",
    "ethers",
    "foundry",
    "web3",
    "dapp",
    "cryptocurrency",
    "automated-trading",
    "portfolio-management"
  ],
  "homepage": "https://github.com/samarabdelhameed/AION_AI_Agent#readme",
  "bugs": {
    "url": "https://github.com/samarabdelhameed/AION_AI_Agent/issues"
  },
  "author": "Samar Abdelhameed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samarabdelhameed/AION_AI_Agent.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@hashgraph/sdk": "^2.75.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.1"
  }
}

{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:automated": "playwright test",
    "test:automated:headed": "playwright test --headed",
    "test:automated:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:strategies": "playwright test src/testing/automated/e2e/strategies-*.spec.ts",
    "test:strategies:headed": "playwright test src/testing/automated/e2e/strategies-*.spec.ts --headed",
    "test:strategies:debug": "playwright test src/testing/automated/e2e/strategies-*.spec.ts --debug",
    "test:strategies:ui": "playwright test src/testing/automated/e2e/strategies-*.spec.ts --ui",
    "test:strategies:report": "playwright test src/testing/automated/e2e/strategies-*.spec.ts --reporter=html",
    "test:strategies:install": "playwright install",
    "test:strategies:codegen": "playwright codegen http://localhost:5173/strategies",
    "test:strategies:run-script": "./scripts/run-strategies-tests.sh"
  },
  "dependencies": {
    "@privy-io/react-auth": "^2.21.2",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.85.0",
    "@tanstack/react-query-devtools": "^5.85.0",
    "@types/node": "^24.2.1",
    "@wagmi/core": "^2.19.0",
    "@web3modal/wagmi": "^5.1.11",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ethers": "^6.15.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.33.3",
    "wagmi": "^2.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",

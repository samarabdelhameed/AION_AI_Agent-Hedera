# Hedera Configuration Example
# Copy this file to .env.hedera and fill in your actual values
# NEVER commit the actual .env.hedera file to git!

# Hedera Network Configuration
HEDERA_NETWORK=testnet
HEDERA_RPC_URL=https://testnet.hashio.io/api

# Account Configuration (REPLACE WITH YOUR VALUES)
PRIVATE_KEY=0xYOUR_ETHEREUM_PRIVATE_KEY_HERE
ADMIN_ADDRESS=0xYOUR_ADMIN_ADDRESS_HERE
EVM_ADDRESS=0xYOUR_EVM_ADDRESS_HERE

# Hedera Account (REPLACE WITH YOUR VALUES)
HEDERA_ACCOUNT_ID=0.0.YOUR_ACCOUNT_ID
HEDERA_PRIVATE_KEY=YOUR_HEDERA_PRIVATE_KEY_HERE

# Service IDs (will be filled after deployment)
HCS_TOPIC_ID=
HFS_FILE_ID=
HTS_TOKEN_ID=
VAULT_CONTRACT_ID=

# BSC Configuration (existing)
BSC_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545
VAULT_CONTRACT_ADDRESS=

# Test Configuration
MIN_TEST_BALANCE=100000000000000000000
VERIFICATION_TIMESTAMP=

# Test Accounts (will be generated automatically)
USER1_ACCOUNT_ID=
USER1_PRIVATE_KEY=

USER2_ACCOUNT_ID=
USER2_PRIVATE_KEY=

AI_AGENT_ACCOUNT_ID=
AI_AGENT_PRIVATE_KEY=

TREASURY_ACCOUNT_ID=
TREASURY_PRIVATE_KEY=

# Account Summary (will be updated automatically)
TOTAL_TEST_ACCOUNTS=
TEST_ACCOUNTS_CREATED=